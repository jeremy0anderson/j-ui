Wrapper, { key: index + word }, words[index].flat().map(function (element, index) {\r\n            return (React.createElement(\"span\", { style: {\r\n                    overflow: \"hidden\",\r\n                    display: \"inline-flex\",\r\n                }, key: index },\r\n                React.createElement(motion.span, { style: { display: \"flex\", overflow: \"visible\" }, variants: item }, element)));\r\n        })));\r\n    })));\r\n};\r\nvar AnimatedText = function (props) {\r\n    return (React.createElement(motion.div, { style: props.style ? props.style : {}, initial: \"hidden\", animate: \"visible\", variants: variants.container },\r\n        React.createElement(AnimatedTextInner, { type: props.type, text: props.text })));\r\n};\r\nexport { AnimatedText };\r\n//# sourceMappingURL=AnimatedTxt.js.map","references":["/Users/JADev/Developer/GitHub/jeremy0anderson/Repositories/j-ui/jda-ui/node_modules/@types/react/index.d.ts","/Users/JADev/Developer/GitHub/jeremy0anderson/Repositories/j-ui/jda-ui/node_modules/framer-motion/dist/index.d.ts"],"map":"{\"version\":3,\"file\":\"AnimatedTxt.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../src/Components/AnimatedTxt.tsx\"],\"names\":[],\"mappings\":\"AAAA,OAAO,KAAK,KAAK,MAAM,OAAO,CAAC;AAC/B,OAAO,EAAE,MAAM,EAAE,MAAM,eAAe,CAAC;AAEvC,IAAM,QAAQ,GAAG;IACb,SAAS,EAAC;QACN,MAAM,EAAE,EAAE,OAAO,EAAE,CAAC,EAAE,CAAC,EAAE,GAAG,EAAC;QAC7B,OAAO,EAAE;YACL,OAAO,EAAE,CAAC;YACV,CAAC,EAAC,CAAC;YACH,UAAU,EAAE;gBACR,aAAa,EAAE,GAAG;gBAClB,eAAe,EAAE,IAAI;aACxB;SACJ;KACJ;IACD,IAAI,EAAC;QACD,MAAM,EAAE;YACJ,CAAC,EAAE,OAAO;YACV,UAAU,EAAE,EAAC,IAAI,EAAE,QAAQ,EAAE,QAAQ,EAAE,GAAG,EAAE,IAAI,EAAE,CAAC,IAAI,EAAE,GAAG,EAAE,GAAG,EAAE,CAAC,CAAC;aACpE;SACJ;QACD,OAAO,EAAE;YACL,CAAC,EAAE,CAAC;YACJ,MAAM,EAAE,MAAM;YACd,QAAQ,EAAE,SAAS;YACnB,UAAU,EAAE,EAAC,IAAI,EAAE,QAAQ,EAAG,IAAI,EAAE,CAAC,CAAC,EAAE,GAAG,EAAE,GAAG,EAAE,CAAC,CAAC,EAAE,QAAQ,EAAE,GAAG,EAAE,MAAM,EAAC,GAAG,EAAE,eAAe,EAAE,GAAG,EAAC;SACzG;KACJ;CACJ,CAAA;AACD,IAAM,OAAO,GAAG,UAAC,KAAS;IACtB,